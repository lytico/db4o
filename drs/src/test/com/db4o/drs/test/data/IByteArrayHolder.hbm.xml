<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="property" default-lazy="false" default-cascade="save-update">
	<class name="com.db4o.drs.test.data.IByteArrayHolder">
		<id column="typed_id" type="long">
			<generator class="native"/>
		</id>

		<discriminator/>

		<property name="bytes"/>
		
		<subclass name="com.db4o.drs.test.data.ByteArrayHolder"/>
	</class>
</hibernate-mapping>
